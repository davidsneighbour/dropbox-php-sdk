    "license": "MIT",
    "authors": [
        {
            "name": "Kunal Varma",
            "email": "kunalvarma05@gmail.com"
        },
        {
            "name": "Patrick Kollitsch",
            "email": "patrick@davids-neighbour.com"
        }
